{
    "id": "hf-client-inference:1.0.0",
    "image": "ritualnetwork/hf_client_inference_service:1.0.0",
    "description": "HuggingFace Client Inference Service",
    "command": "--bind=0.0.0.0:3000 --workers=${NUM_WORKERS}",
    "env": {
        "HF_TOKEN": "${HF_TOKEN}",
        "TEST_ENV": "${TEST_ENV}"
    },
    "defaults": {
        "NUM_WORKERS": 2,
        "TEST_ENV": "false"

    },
    "required": ["HF_TOKEN"]
}
