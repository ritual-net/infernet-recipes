{
    "id": "onnx-inference:1.0.0",
    "image": "ritualnetwork/onnx_inference_service:1.0.0",
    "description": "ONNX Inference Service",
    "command": "--bind=0.0.0.0:3000 --workers=${NUM_WORKERS}",
    "env": {
        "MODEL_SOURCE": "${MODEL_SOURCE}",
        "LOAD_ARGS": "${LOAD_ARGS}",
        "TEST_ENV": "${TEST_ENV}"
    },
    "defaults": {
        "NUM_WORKERS": 2,
        "TEST_ENV": "false"
    },
    "required": []
}
