{
  "config": {
    "manage_containers": true,
    "startup_wait": 5,
    "server": {
      "port": 4000
    },
    "chain": {
        "enabled": true,
        "trail_head_blocks": 3,
        "rpc_url": "https://base-sepolia-rpc.publicnode.com",
        "registry_address": "0x3B1554f346DFe5c482Bb4BA31b880c1C18412170",
        "wallet": {
            "max_gas_limit": 5000000,
            "private_key": "",
            "payment_address": "",
            "allowed_sim_errors": []
        },
        "snapshot_sync": {
            "sleep": 1,
            "batch_size": 1800,
            "starting_sub_id": 0
        }
    },
    "docker": {
      "username": "",
      "password": ""
    },
    "redis": {
      "host": "redis",
      "port": 6379
    },
    "forward_stats": false,
    "containers": []
  },
  "inputs": [
    {
      "id": "private_key",
      "path": "chain.wallet.private_key",
      "description": "Private key for the wallet",
      "type": "string",
      "required": true
    },
    {
      "id": "payment_address",
      "path": "chain.wallet.payment_address",
      "description": "Payment address for the wallet",
      "type": "string",
      "required": false
    }
  ]
}
